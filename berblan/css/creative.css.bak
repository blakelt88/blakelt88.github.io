body,
html {
  width: 100%;
}

body {
	margin-top:25px;
}

.video-div {
	height:600px;
}

.threeImgs {
	padding:15px;
	margin-bottom:25px;
	background-repeat:no-repeat;
  background-position: center center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
	width:100%;
}

#twitter-widget-0{
   width: 100% !important;  

}

#twitter-widget-1{
   width: 100% !important;  

}

.timeline-header, .timeline-footer
{
          display:none ;
}



.threeImgs .row {
	width:60%;
	margin:0 auto;
}

.center-img img {
	display:block;
	margin:0 auto;
}

.threeImgs img {
	padding:25px;
	height:384px;
}

.my-auto {
	width:50% !important;
}

body {
  font-family: 'Abel', sans-serif !important;
  line-height:.75 !important;
  background:url("../img/white.jpg");
  background-repeat:no-repeat;
  background-position: center center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}


 

hr {

    background-color:#8c8f78 !important;

    margin-top: 20px !important;

    margin-bottom: 20px !important;

    width: 75% !important;

}



a {
  color: #E6A8D7 !important;
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  transition: all 0.2s;

}

a:hover {
  color: #E6A8D7 !important;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: 'Roboto', sans-serif !important;
}

#twitter {
	
}

.bg-primary {
  background-color: #E6A8D7 !important;
}

.bg-dark {
  background-color: #212529 !important;
}

.text-faded {
  color: rgba(255, 255, 255, 0.7);
}

section {
  padding: 5rem 0;
}

.section-heading {
  margin-top: 0;
}

::-moz-selection {
  color: #fff;
  background: #212529;
  text-shadow: none;
}

::selection {
  color: #fff;
  background: #212529;
  text-shadow: none;
}

img::selection {
  color: #fff;
  background: #fafafa;
}

img::-moz-selection {
  color: #fff;
  background: #fafafa;
}

#mainNav {
  background-color: #fff;
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  transition: all 0.2s;
  z-index:999999;
}

#mainNav .navbar-brand {
  text-transform: uppercase;
  color: black !important;
  font-family: 'Philosopher', sans-serif;
  text-shadow:0 0 1px #fafafa !important;
  font-size: 35px;
}

#mainNav .navbar-nav > li.nav-item > a.nav-link,
#mainNav .navbar-nav > li.nav-item > a.nav-link:focus {
  font-size: 18px;
  -webkit-font-smoothing: antialiased;
  text-transform: uppercase;
  font-family: 'Josefin Sans', sans-serif;
  color: #212529 !important;
}



#mainNav .navbar-nav > li.nav-item > a.nav-link.active:hover,
#mainNav .navbar-nav > li.nav-item > a.nav-link:focus.active:hover {
  background-color: #fafafa;
}

@media (min-width: 992px) {
	
	#twitter {
		display:none;
	}
	
  #mainNav {
    border-color: #fafafa;
    background-color: #fafafa;
  }
  #mainNav .navbar-brand {
    color: rgba(255, 255, 255, 0.7);
  }
  #mainNav .navbar-brand:focus, #mainNav .navbar-brand:hover {
    color: #fff;
  }
  #mainNav .navbar-nav > li.nav-item > a.nav-link {
    padding: 0.5rem 1rem;
  }
  #mainNav .navbar-nav > li.nav-item > a.nav-link,
  #mainNav .navbar-nav > li.nav-item > a.nav-link:focus {
    color: rgba(255, 255, 255, 0.7);
  }
  #mainNav .navbar-nav > li.nav-item > a.nav-link:hover,
  #mainNav .navbar-nav > li.nav-item > a.nav-link:focus:hover {
    color: #fff;
  }
  #mainNav.navbar-shrink {
    border-bottom: 1px solid #fafafa;
    background-color: #fafafa;
  }
  #mainNav.navbar-shrink .navbar-brand {
    color: black;
  }
  #mainNav.navbar-shrink .navbar-brand:focus, #mainNav.navbar-shrink .navbar-brand:hover {
    color: #212529 !important;
  }
  #mainNav.navbar-shrink .navbar-nav > li.nav-item > a.nav-link,
  #mainNav.navbar-shrink .navbar-nav > li.nav-item > a.nav-link:focus {
    color: #212529 !important;
  }
  #mainNav.navbar-shrink .navbar-nav > li.nav-item > a.nav-link:hover,
  #mainNav.navbar-shrink .navbar-nav > li.nav-item > a.nav-link:focus:hover {
    color: #212529 !important;
  }
}

header.masthead {
  padding-top: 10rem;
  padding-bottom: calc(10rem - 56px);
  background-position: center center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  width:100% !important;
  background:url("../img/white3.png");
  background-repeat:no-repeat;
	background-position:center;
}

header.masthead hr {
  margin-top: 30px;
  margin-bottom: 30px;
}

header.masthead h1 {
  font-size: 2rem;
}

header.masthead p {
  font-weight: 300;
  
}

.younowlink {
	width:100%;
	height:50px;
}

.younowlink a {
	font-size:35px;
	text-decoration:none;
	text-transform:uppercase;
	font-weight:bold;
	color:#FFB6C1 !important;
	padding:25px;
	text-align:center !important;
	font-family: 'Gaegu', cursive;
}

.younowlink a:hover {
	color:#E6A8D7 !important;
}

iframe {
	width:750px;
	max-width:100%;
	height:500px;
}

.twitter-feed {
	height:75px;
	text-align:center;
}

.twitter-feed a {
	padding:15px 75px 15px 75px;
	background:white;
	border:solid 2px #ed8098;
	color:#ed8098 !important;
	font-weight:bold;
	margin:25px;
	text-decoration:none;
}

.twitter-feed a:hover {
	letter-spacing:2px;
	background:#73624d;
	border:solid 2px #73624d;
	color:white !important;
}

.grid-Widget-footer {
	display:none !important;
}


#contact {
	height:500px;
	width:100%;
	background:#fafafa;
}

hr {
	color:#898c77 !important;
}

.subscribe div{
	margin:0 auto;
	display:block;
	text-align:center;
	height:100px;
}

.subscribe input[type="text"] {
	height:48px;
	margin-right:1px;
	width:50%;
}

.subscribe input[type="button"] {
	    height: 50px;
    background: white;
    color: #ed8098 !important;
    font-weight: bold;
    border: solid 2px #ed8098 !important;
}

.subscribe input[type="button"]:hover {
	background:#73624d;
	border:solid 2px #73624d !important;
	color:white !important;
}


.social-media {
	height:150px;
	text-align:center;
}

.social-media a {
	padding:25px;
	text-align:center;
}

.copyright {
	text-align:center;
	height:50px;
	margin-top:50px;
}

.hide {
	display:none;
}

.credit {
	text-align:center;
}

@media (max-width:941px) {

	
	header.masthead {
    height: 100vh;
    min-height: 650px;
    padding-top: 0;
    padding-bottom: 0;
  }
  header.masthead h1 {
    font-size: 3rem;
  }
	
	#twitter {
		display:none;
	}
	
	iframe {
		width:100% !important;
	}
	
	.my-auto {
	width:100% !important;
}
}

@media (min-width: 768px) {
  header.masthead p {
    font-size: 1.15rem;
  }
  
  .hidden {
    	display:none;
    }     
    
    #twitter {
    	display:none;
    } 
    
    .portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-category {
    font-size: 35px;
  }
  .portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-name {
    font-size: 22px;
  }
  
  .hidden {
    	display:"";
    }
}

@media (min-width: 1200px) {
  header.masthead h1 {
    font-size: 4rem;
  }
}

.service-box {
  max-width: 400px;
}

.portfolio-box {
  position: relative;
  display: block;
  max-width: 650px;
  margin: 0 auto;
}

.portfolio-box .portfolio-box-caption {
  position: absolute;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100%;
  text-align: center;
  opacity: 0;
  color: #fff;
  background: #E6A8D7;
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  transition: all 0.2s;
}

.portfolio-box .portfolio-box-caption .portfolio-box-caption-content {
  position: absolute;
  top: 50%;
  width: 100%;
  transform: translateY(-50%);
  text-align: center;
}

.portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-category,
.portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-name {
  padding: 0 15px;
  font-family: 'Open Sans', 'Helvetica Neue', Arial, sans-serif;
}

.portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-category {
  font-size: 14px;
  font-weight: 600;
  text-transform: uppercase;
}

.portfolio-box .portfolio-box-caption .portfolio-box-caption-content .project-name {
  font-size: 18px;
}

.portfolio-box:hover .portfolio-box-caption {
  opacity: .75;
}

.portfolio-box:focus {
  outline: none;
}

.text-primary {
  color: #E6A8D7 !important !important;
}

.btn {
  font-weight: 700;
  text-transform: uppercase;
  border: none;
  border-radius: 300px;
  font-family: 'Open Sans', 'Helvetica Neue', Arial, sans-serif;
}

.btn-xl {
  padding: 1rem 2rem;
}

.btn-primary {
  background-color: #E6A8D7 !important;
  border-color: #E6A8D7 !important;
}

.btn-primary:hover, .btn-primary:focus, .btn-primary:active {
  color: #fff;
  background-color: #ee4b28 !important;
}

.btn-primary:active, .btn-primary:focus {
  box-shadow: 0 0 0 0.2rem rgba(240, 95, 64, 0.5) !important;
} 

@media only screen and (max-width: 600px) {
	
		body {
	margin-top:0px;
}

.video-div {
	height:500px;
}
    .hidden {
    	display:"";
    }  
    
    #twitter {
    	display:block;
    }     
    
    #portfolio {
    	display:none;
    }
    
    .my-auto iframe {
    	height:350px;
    }              
    
    .threeImgs .row {
    width: 100%;
    margin: 0 auto;
    padding:0;
		}
		
		.threeImgs img {
			padding:0;
			width:85%;
			margin:0 auto;
			margin-bottom:15px;
			display:block;
		}
		
		.younowlink a {
			font-size:22px;
		}
		
		.container-fluid {
			padding:0;
		}
		
		.row {
			margin:0;
			padding:0;
		}
		
		.col-lg-12 {
			padding:0;
			margin:0;
		}
		
		.col-lg-4 {
			padding:0;
			margin:0;
		}
		
		header.masthead {
			padding-top:5rem;
		}
		
		.twitter-feed a {
    padding: 15px 65px 15px 65px;
  }
  
  .twitter-feed a:hover {
    letter-spacing:0;
    
  }
  
  .twitter-feed {
  	height:50px;
  }
  
  #portfolio {
  	display:none;
  }
  
  .subscribe input[type="text"] {
  	width:100%;
  }
  
  .subscribe input[type="text"]::placeholder {
  	text-align:center;
  }
  
  .subscribe input[type="button"] {
  	width:100%;
  	margin-top:6.5px;
  	margin-bottom:5px;
  }
  
  .social-media {
  	margin-top:45px;
  }
  
  .social-media a {
  	padding:5px;
  }
  
  li.nav-item a {
  	padding:15px;
  	text-align:center;
  	border:1px #949484 solid;
  }
  
  .navbar.navbar-expand-lg.navbar-light.fixed-top {
  	padding:0;
  }
  
  #mainNav a {
  	padding:5px;
  }
  
  #mainNav button {
  	margin:5px;
  }
  
  .nav-link.js-scroll-trigger {
  	padding:15px !important;
  }
  
  .credit a {
  	line-height:1.5;
  }
  
  #contact {
  	padding-top:25px;
  }
}    
                            
